#importando dados ----
dados <- read_excel("C:/Users/diogo/OneDrive/Notebook/dados.xlsx")
head(dados)
## Calcular o desvio padrão por espécie
dados2 <- dados %>%
dplyr::group_by(acude) %>%
dplyr::summarise(media = mean(clorofila, na.rm = TRUE),
desvio = sd(clorofila,  na.rm = TRUE))
View(dados2)
